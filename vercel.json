  "outputDirectory": ".next",
  "framework": "nextjs",
  "git": {
    "deploymentEnabled": {
      "main": true,
      "develop": false
    }
  },
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 10
